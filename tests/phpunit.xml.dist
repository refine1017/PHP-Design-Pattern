<phpunit bootstrap="./Bootstrap.php" colors="true">
    <testsuites>
        <testsuite name="PHP-Design-Pattern Test Suite">
            <directory>./FactoryTest</directory>
        </testsuite>
    </testsuites>
    <filter>
        <whitelist>
            <directory>../src/FactoryTest/</directory>
            <exclude>
                <directory>../vendor/</directory>
            </exclude>
        </whitelist>
    </filter>
</phpunit>